<script language="JavaScript" type="text/javascript">
    function trackDownload(downloadForm,category,action){
        for(i=0;i<downloadForm.length;i++){
            var fileExtensions = {'zip':1,'tar.gz':2,'tar.bz2':3,'gz':4,'bz2':5};
            if(downloadForm[i].type == 'select-one'){
                pageTracker._trackPageview('/virtual/download-magento/'+downloadForm[i].value);

                var currValue = downloadForm[i].value;
                var selectedVersion = '';
                var currVersion = currValue.split("/");
                var fileExtension = 0;
                selectedVersion = currVersion[0];
                if(downloadForm[i].options[downloadForm[i].selectedIndex].className) fileExtension = fileExtensions[downloadForm[i].options[downloadForm[i].selectedIndex].className];
                if(currVersion[0] == 'tools') selectedVersion = '1.0';
                if(currVersion[0] == 'zend') selectedVersion = '1.0.0';

                pageTracker._trackEvent(category, action, selectedVersion, fileExtension);
            }
        }
    }
    function trackEnRegister(but){pageTracker._trackPageview('/virtual/enterprise-register/'+but);}
</script>

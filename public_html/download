<script language="JavaScript" type="text/javascript">
function changeDownloadTab(tab){
    if($('Overlay')) $('Overlay').style.height = '1px';
    var tabs = new Array("download-simple", "download-archives", "download-getstarted");
    for(i=0;i<tabs.length;i++){
        document.getElementById(tab).style.display = "block";
        document.getElementById(tab + "-tab").className = "hover";
        if(tab != tabs[i]){
            document.getElementById(tabs[i]).style.display = "none";
            document.getElementById(tabs[i] + "-tab").className = "";
        }
    }
    if($('Overlay')) $('Overlay').style.height = window.getScrollHeight()+'px';
}
</script>

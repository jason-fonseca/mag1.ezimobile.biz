<a href="http://www.magentocommerce.com/download/login_form" id="rb1" rel="width:794,height:443,ajax:true" class="rb"></a>
<script type="text/javascript">
    var activeForm = false;
    var registerBox = {};
    window.addEvent('domready', function(){
        registerBox = new MultiBox('rb', {useOverlay:true,showNumbers:false,showControls:false,openFromLink:false});
        var downloadLinks = $$('.download-link');
        downloadLinks.each(function(a){
            a.href = 'http://www.magentocommerce.com/download/login_form';
            a.addEvent('click', function(e){
                new Event(e).stop();
                var form = $(a).getParent('form');
                if(checkDownloadSelect(form)){
                    registerBox.open($('rb1'));
                    activeForm = form;
                }
            });
        });
    });
    function downloadMagento(){
        registerBox.close();
        this.activeForm.onsubmit();
        this.activeForm.submit();
    }
</script>

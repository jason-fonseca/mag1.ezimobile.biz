<script type="text/javascript">
    if(!$('head_cart_link')) $('right_nav_block').addClass('login-no-cart');

    var eff_duration = 300;
    var menuSlide = new Fx.Slide('top_menu', {
        duration:eff_duration,
        onComplete:function(){
            if($('top_menu').style.visibility == 'hidden'){
                $('top_menu').style.visibility = 'visible';
                $('top_menu').effect('opacity', {duration: 0}).start(1,0);
            } 
        }
    });
    menuSlide.slideOut();
    
    var menuSlide2 = new Fx.Slide('search_top_menu', {
        duration:eff_duration,
        onComplete:function(){
            if($('search_top_menu').style.visibility == 'hidden'){
                $('search_top_menu').style.visibility = 'visible';
                $('search_top_menu').effect('opacity', {duration: 0}).start(1,0);
            } 
        }
    });
    menuSlide2.slideOut();

    $('log_in_lnk').addEvent('click', function(e){
            new Event(e).stop();
            if($('log_in_tab').hasClass('active')){
                $('log_in_tab').removeClass('active');
                menuSlide.slideOut();
                $('top_menu').effect('opacity', {duration: eff_duration}).start(1,0);
            }else{
                $('search_tab').removeClass('active');
                menuSlide2.slideOut();
                $('search_top_menu').effect('opacity', {duration: eff_duration}).start(1,0);
                
                $('log_in_tab').addClass('active');
                menuSlide.slideIn();
                $('top_menu').effect('opacity', {duration: eff_duration}).start(0,1);
            }
        }.bind(this)
    );

    $('search_lnk').addEvent('click', function(e){
            new Event(e).stop(); 
            if($('search_tab').hasClass('active')){
                $('search_tab').removeClass('active');
                menuSlide2.slideOut();
                $('search_top_menu').effect('opacity', {duration: eff_duration}).start(1,0);
            }else{
                $('log_in_tab').removeClass('active');
                menuSlide.slideOut();
                $('top_menu').effect('opacity', {duration: eff_duration}).start(1,0);
                
                $('search_tab').addClass('active');
                menuSlide2.slideIn();
                $('search_top_menu').effect('opacity', {duration: eff_duration}).start(0,1);
            }
        }.bind(this)
    );
    
    $$('.fancy-chb').each(function(chbox){
        if(chbox.checked == true){chbox.getParent('span').addClass('fancy-chb-wrap-over');}
        chbox.addEvent('change', function(e){
            if(chbox.checked == true){chbox.getParent('span').addClass('fancy-chb-wrap-over');}else{chbox.getParent('span').removeClass('fancy-chb-wrap-over');}
        });
    });
    $('unavtip').effect('opacity', {duration: 0}).start(1,0);
    $('unavtip').style.visibility = 'visible';
    $('homelink').addEvent('mouseover', function(e){
        $('unavtip').effect('opacity', {duration: 150}).start(0,1);
    }.bind(this));
    $('homelink').addEvent('mouseout', function(e){
        $('unavtip').effect('opacity', {duration: 150}).start(1,0);
    }.bind(this));
    
    var refnQuery = '';    
    
        
        
    
    
    $('unav_search').addEvent('submit', function(e){
        new Event(e).stop();
        var srchField = $('top_search');
        var srchQuery = '';
        if(srchField.value != '' && srchField.value != srchField.title){
            srchQuery = str_replace(' ', '+',srchField.value);
            srchField.value = '';
            document.location = 'http://www.magentocommerce.com/search/results/?q=' + srchQuery + (refnQuery ? '#' + refnQuery : '');
            $('search_tab').removeClass('active');
            menuSlide2.slideOut();
            $('search_top_menu').effect('opacity', {duration: eff_duration}).start(0);
        }
    });

</script>

